{
  "name": "proof-of-your-life",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write projects/",
    "build:admin": "ng build admin",
    "watch:admin": "ng build admin --watch --configuration development",
    "build:common": "ng build common && rm -rf functions/src/common/src && cp -r projects/common/src functions/src/common/src",
    "build:lp": "ng build lp",
    "watch:lp": "ng build lp --watch --configuration development",
    "build:shared": "ng build shared",
    "watch:shared": "ng build shared --watch --configuration development",
    "build:web": "ng build web",
    "watch:web": "ng build web --watch --configuration development",
    "encode-service-account-key:dev": "cat service-account-key.dev.json | base64 > service-account-key.dev.encoded.txt",
    "encode-service-account-key:test": "cat service-account-key.test.json | base64 > service-account-key.test.encoded.txt",
    "encode-service-account-key:stage": "cat service-account-key.stage.json | base64 > service-account-key.stage.encoded.txt",
    "encode-service-account-key:prod": "cat service-account-key.prod.json | base64 > service-account-key.prod.encoded.txt",
    "decode-service-account-key:dev": "cat service-account-key.dev.encoded.txt | base64 -d > service-account-key.dev.decoded.txt",
    "decode-service-account-key:test": "cat service-account-key.test.encoded.txt | base64 -d > service-account-key.test.decoded.txt",
    "decode-service-account-key:stage": "cat service-account-key.stage.encoded.txt | base64 -d > service-account-key.stage.decoded.txt",
    "decode-service-account-key:prod": "cat service-account-key.prod.encoded.txt | base64 -d > service-account-key.prod.decoded.txt"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.7",
    "@angular-eslint/builder": "17.1.1",
    "@angular-eslint/eslint-plugin": "17.1.1",
    "@angular-eslint/eslint-plugin-template": "17.1.1",
    "@angular-eslint/schematics": "17.1.1",
    "@angular-eslint/template-parser": "17.1.1",
    "@angular/cli": "^17.0.7",
    "@angular/compiler-cli": "^17.0.0",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "firebase-tools": "^13.0.3",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.0.0",
    "typescript": "~5.2.2"
  }
}
